@model UpdateServiceDto

@{
    ViewData["Title"] = "UpdateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Update Service</h4>
                        <form method="post">
                            <div class="form-group">
                                <input type="hidden" asp-for="Id" />
                                <label for="exampleInputTitle">Title</label>
                                <input type="text" class="form-control" id="exampleInputName" placeholder="Enter Your Title" asp-for="Title">
                                <label for="exampleInputDescription">Description</label>
                                <input type="text" class="form-control" id="exampleInputDescription" placeholder="Enter Your Description" asp-for="Description">
                                <label for="exampleInputIconUrl">IconUrl</label>
                                <input type="text" class="form-control" id="exampleInputIconUrl" placeholder="Enter Your IconUrl" asp-for="IconUrl">
                            </div>
                            <button type="submit" class="btn btn-primary waves-effect waves-light">Update</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>